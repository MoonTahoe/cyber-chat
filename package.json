{
  "name": "cyber-chat",
  "version": "0.1.3",
  "description": "An interactive chat application designed to teach Universal React/Redux",
  "author": "Alex Banks <alex@moonhighway.com> (http://www.moonhighway.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MoonTahoe/cyber-chat/issues"
  },
  "homepage": "https://github.com/MoonTahoe/cyber-chat#readme",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "dev": "open http://localhost:3000 & webpack -w --progress & ./node_modules/.bin/nodemon ./server --exec babel-node",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha -- --require ./test/setup.js --recursive",
    "shallow-test": "mocha --compilers js:babel-core/register --recursive --watch",
    "posttest": "npm run lint",
    "coverage": "open ./coverage/lcov-report/index.html",
    "prestart": "webpack",
    "start": "./node_modules/.bin/babel-node ./server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoonTahoe/cyber-chat.git"
  },
  "keywords": [
    "Tutorial",
    "Universal",
    "React",
    "Redux",
    "Learning",
    "React"
  ],
  "dependencies": {
    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.2",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-istanbul": "^0.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-rewire": "^1.0.0-rc-3",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cheerio": "^0.20.0",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.8.5",
    "jsdom": "^9.2.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.9.1",
    "rewire": "^2.5.1",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "supertest": "^1.2.0",
    "webpack": "^1.13.1"
  }
